{
    "name": "@pomdtr/val-town-heynote",
    "version": "0.1.0",
    "exports": {
        ".": "./mod.ts"
    },
    "tasks": {
        "embed": "deno run -A ./embedder.ts build",
        "publish": "deno task embed && deno publish",
        "start": "deno serve ./main.ts"
    },
    "publish": {
        "include": [
            "./mod.ts",
            "./embed/**/*"
        ]
    },
    "imports": {
        "mime": "npm:mime@^4.0.3"
    }
}
